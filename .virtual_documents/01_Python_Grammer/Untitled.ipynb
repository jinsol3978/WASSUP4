import numpy as np
a=np.arange(15)
a=a.reshape(3,5,order='F')
print(a)


b=np.array([-1,2,5,-3,0,7,-9,4,-4,-7]) # List
print(type(b))
print(b)


t=(10,20,30,40,50) # tuple
print(type(t))
t=np.array(t)
print(type(t))


s= {10,20,30,40,50} # set
print(type(s))
s=np.array(s)
print(type(s))


# list에서 numpy로 변환한 변수 b 활용
b=np.array([-1,2,5,-3,0,7,-9,4,-4,-7])
print(b)

# abs : 절대값 구하는 함수
print(np.abs(b))


print(b.shape)
print(b.ndim)
print(b.dtype)
print(b)

