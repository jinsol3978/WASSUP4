    'tel':['1111-2222','2222-1111','3333-1111','2222-3333']
}
print(type(d))
print(d)


df=pd.DataFrame(d)  #Dictionary->DataFrame
print(df)           #DataFrame 출력 시 테이블 형태로 출력 가능


print(df.index)
print(df.columns)
print(df.values)


# index와 colums에 특정 이름 부여 가능
df.index.name = 'Num'
df.columns.name='User'
print(df)

print(df.index)
print(df.columns)


